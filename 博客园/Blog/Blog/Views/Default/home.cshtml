
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <link href="~/css/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/css/zy.layout.css" rel="stylesheet" />
    <link href="~/css/zy.form.css" rel="stylesheet" />
    <link href="~/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/css/mya.css" rel="stylesheet" />
    <link href="~/css/zy.menu.css" rel="stylesheet" />
    <script src="~/scripts/jquery-2.1.4.min.js"></script>
    <script src="~/scripts/my.js"></script>
</head>


<body >
    <div class="dvheader">
        <div class="dvheadertools">
            <span class="headerspantitle"><a href="inde">博客园</a></span>
            <ul class="headerultools" >
                <li  class="headerlitools_info headerlitools on" style="background-color: #74abda;text-align:center;">
                    @foreach (var i in ViewBag.Show)
                    {
                        <p class="m">@i.uname</p>

                    }

                </li>
                <li class="headerlitools_info headerlitools tw" style="background-color: #74abda;text-align:center">
                    @foreach (var i in ViewBag.Show)
                    {
                        <p class="m">@i.nickname</p>

                    }

                </li>

                 
                
            </ul>
        </div>
    </div>

        <div class="dvcontent">

            <ul class="ulleftmenu" style="border-right: 1px solid #ddd;">

                <li class="limenuitem" >
                    <h3 style="text-align:center">操 作</h3>
                    <ul class="ulleftsubitems">
                        <a href="newsType" target="right">
                            <li>资讯类别</li>
                        </a>
                        <a href="news" target="right">
                            <li>资讯管理</li>
                        </a>
                        <a href="link" target="right">
                            <li>友情链接</li>
                        </a>
                    </ul>
                </li>
            </ul>
            <div style="position: absolute; left: 191px; right: 20px; ">
                <iframe  src="@ViewBag.page" scrolling="no" width="100%"  height="1200px" name="right" ></iframe>




            </div>
        </div>

        <script src="~/scripts/jquery-1.7.2.min.js" type="text/javascript"></script>
        <script src="~/scripts/plugs/Jqueryplugs.js" type="text/javascript"></script>
        <script src="~/scripts/_layout.js"></script>

      
</body>

</html>